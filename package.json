{
  "name": "vue-tools",
  "version": "0.0.35",
  "description": "vue tools",
  "keywords": [
    "vue",
    "vui",
    "vue-tools",
    "vue-components"
  ],
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2046/vue-tools.git"
  },
  "author": "2046",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/2046/vue-tools/issues"
  },
  "homepage": "https://github.com/2046/vue-tools",
  "dependencies": {
    "Imager": "^0.1.2",
    "vue": "^2.0.0",
    "vue-resource": "^1.0.3",
    "vue-router": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.18.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.19.0",
    "isparta-loader": "^2.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-webpack": "^1.8.0",
    "koa": "^1.1.0",
    "koa-proxy": "^0.6.0",
    "koa-webpack-dev-middleware": "^1.2.1",
    "koa-webpack-hot-middleware": "^1.0.3",
    "mocha": "^3.1.0",
    "postcss-px2rem": "^0.3.0",
    "shelljs": "^0.7.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^2.0.6",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^9.7.0",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.13.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.15.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "scripts": {
    "start": "node examples/server.js",
    "cli": "./bin/cli.js",
    "plugin": "npm run cli -- plugin $1",
    "directive": "npm run cli -- directive $1",
    "component": "npm run cli -- component $1",
    "build": "./node_modules/.bin/babel index.js -d dist && ./node_modules/.bin/babel plugin -d dist/plugin && cp -r component/ dist/component/ && ./node_modules/.bin/babel component --out-dir dist/component",
    "prepublish": "npm run build",
    "test": "node_modules/.bin/karma start tests/karma.conf.js"
  }
}
